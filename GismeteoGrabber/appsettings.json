{
  "grabPeriodInMinutes": "1440", // minutes in a day
  "gismeteo": {
    "baseUrl": "https://www.gismeteo.ru/",
    "cityFrameRegex": "<!-- City frame -->.*<!-- End City frame -->",
    "geoObject": {
      "xPath": "//a",
      "idAttr": "data-id",
      "nameAttr": "data-name",
      "urlAttr": "data-url"
    },
    "maxTemperatureXPath": "//div[@class='maxt']//span[@class='unit unit_temperature_c']/text()",
    "minTemperatureXPath": "//div[@class='mint']//span[@class='unit unit_temperature_c']/text()",
    "precipitationXPath": "//div[@class='w_prec__value']/text()",
    "tenDaysUrlPart": "10-days"
  },
  "ConnectionStrings": {
    "weather": "server=127.0.0.1;user id=user;password=password;port=3306;database=weather;"
  },
  "system": {
    "degreeOfParallelism": 4
  }
}